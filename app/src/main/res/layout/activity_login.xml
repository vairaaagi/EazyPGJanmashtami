<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android.support.design="http://schemas.android.com/apk/res-auto">

    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android.support.design:cardBackgroundColor="?attr/actionModeSplitBackground" >

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginStart="16dp"
            android.support.design:srcCompat="@drawable/ic_arrow_back_black_24dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginStart="48dp"
            android:fontFamily="@font/productsans"
            android:text="Register"
            android:textColor="@android:color/white"
            android:textSize="24dp" />
    </android.support.v7.widget.CardView>

    <android.support.design.widget.TextInputLayout
        android:id="@+id/ownerNameTIL"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:clipChildren="false"
        android:hint="Name"
        android.support.design:hintAnimationEnabled="true"
        android.support.design:hintEnabled="true"
        android.support.design:hintTextAppearance="@style/TextLabel4"
        android.support.design:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="261dp">


        <android.support.design.widget.TextInputEditText
            android:id="@+id/ownerNameEditText"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="3dp"
            android:background="@drawable/text_bg_blue"
            android:fontFamily="@font/productsans"
            android:inputType="textPersonName"
            android:maxLength="50"
            android:text=""
            android:textColor="#424242"
            android:textSize="16dp"
            android:translationY="3dp"
            android.support.design:layout_constraintEnd_toEndOf="parent"
            android.support.design:layout_constraintHorizontal_bias="0.758"
            android.support.design:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="284dp" />

    </android.support.design.widget.TextInputLayout>

    <android.support.design.widget.TextInputLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:clipChildren="false"
        android:hint="Contact"
        android.support.design:hintAnimationEnabled="true"
        android.support.design:hintEnabled="true"
        android.support.design:hintTextAppearance="@style/TextLabel4"
        android.support.design:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="329dp">


        <android.support.design.widget.TextInputEditText
            android:id="@+id/contactEditText"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="3dp"
            android:background="@drawable/text_bg_blue"
            android:fontFamily="@font/productsans"
            android:inputType="textPersonName"
            android:maxLength="50"
            android:text=""
            android:textColor="#424242"
            android:textSize="16dp"
            android:translationY="3dp"
            android.support.design:layout_constraintEnd_toEndOf="parent"
            android.support.design:layout_constraintHorizontal_bias="0.758"
            android.support.design:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="284dp" />

    </android.support.design.widget.TextInputLayout>

    <android.support.design.widget.TextInputLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:clipChildren="false"
        android:hint="Organisation"
        android.support.design:hintAnimationEnabled="true"
        android.support.design:hintEnabled="true"
        android.support.design:hintTextAppearance="@style/TextLabel4"
        android.support.design:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="423dp">


        <android.support.design.widget.TextInputEditText
            android:id="@+id/organisationEditText"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="3dp"
            android:background="@drawable/text_bg_blue"
            android:fontFamily="@font/productsans"
            android:inputType="textPersonName"
            android:maxLength="50"
            android:text=""
            android:textColor="#424242"
            android:textSize="16dp"
            android:translationY="3dp"
            android.support.design:layout_constraintEnd_toEndOf="parent"
            android.support.design:layout_constraintHorizontal_bias="0.758"
            android.support.design:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="284dp" />

    </android.support.design.widget.TextInputLayout>

    <android.support.design.widget.TextInputLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:clipChildren="false"
        android:hint="Date of Birth"
        android.support.design:hintAnimationEnabled="true"
        android.support.design:hintEnabled="true"
        android.support.design:hintTextAppearance="@style/TextLabel4"
        android.support.design:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="423dp">


        <android.support.design.widget.TextInputEditText
            android:id="@+id/dateOfBirthEditText"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="3dp"
            android:background="@drawable/text_bg_blue"
            android:fontFamily="@font/productsans"
            android:inputType="textPersonName"
            android:maxLength="50"
            android:text=""
            android:textColor="#424242"
            android:textSize="16dp"
            android:translationY="3dp"
            android.support.design:layout_constraintEnd_toEndOf="parent"
            android.support.design:layout_constraintHorizontal_bias="0.758"
            android.support.design:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="284dp" />

    </android.support.design.widget.TextInputLayout>

    <android.support.design.widget.TextInputLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:clipChildren="false"
        android:hint="folk Guide Name"
        android.support.design:hintAnimationEnabled="true"
        android.support.design:hintEnabled="true"
        android.support.design:hintTextAppearance="@style/TextLabel4"
        android.support.design:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="567dp">


        <android.support.design.widget.TextInputEditText
            android:id="@+id/folkGuideNameEditText"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="3dp"
            android:background="@drawable/text_bg_blue"
            android:fontFamily="@font/productsans"
            android:inputType="textPersonName"
            android:maxLength="50"
            android:text=""
            android:textColor="#424242"
            android:textSize="16dp"
            android:translationY="3dp"
            android.support.design:layout_constraintEnd_toEndOf="parent"
            android.support.design:layout_constraintHorizontal_bias="0.758"
            android.support.design:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="284dp" />

    </android.support.design.widget.TextInputLayout>

    <android.support.design.widget.TextInputLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:clipChildren="false"
        android:hint="Rounds Chanting"
        android.support.design:hintAnimationEnabled="true"
        android.support.design:hintEnabled="true"
        android.support.design:hintTextAppearance="@style/TextLabel4"
        android.support.design:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="619dp">


        <android.support.design.widget.TextInputEditText
            android:id="@+id/roundsChanting"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="3dp"
            android:background="@drawable/text_bg_blue"
            android:fontFamily="@font/productsans"
            android:inputType="textPersonName"
            android:maxLength="50"
            android:text=""
            android:textColor="#424242"
            android:textSize="16dp"
            android:translationY="3dp"
            android.support.design:layout_constraintEnd_toEndOf="parent"
            android.support.design:layout_constraintHorizontal_bias="0.758"
            android.support.design:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="284dp" />

    </android.support.design.widget.TextInputLayout>


    <Button
        android:id="@+id/signUpButton"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="16dp"
        android:background="@drawable/fui_idp_button_background_github"
        android:fontFamily="@font/productsans"
        android:text="Sign Up"
        android:textColor="@android:color/white"
        android:textSize="18dp"
        tools:layout_editor_absoluteX="208dp"
        tools:layout_editor_absoluteY="651dp" />

</LinearLayout>